    "N = int(input())\n",
    "seats = input().strip().split(' ')\n",
    "\n",
    "max_space = 0\n",
    "space = 0\n",
    "for seat in seats:\n",
    "    if(seat == '1'):\n",
    "        max_space = max(max_space,space)\n",
    "        space = 0\n",
    "    else:\n",
    "        space += 1\n",
    "print((max_space + 1)//2)"
